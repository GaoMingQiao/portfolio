{# {% extends 'base.html.twig' %}

{% block title %}Mingqiao GAO{% endblock %}

{% block body %} #}

<section class="py-5">
    <div class="container px-4 px-lg-5 my-5">
        <div class="row gx-4 gx-lg-5 align-items-center my-5">
            {% for produit in produits %}
            <div class="projectItem my-5">
                <div class="col-md-6 picture">
                <img class="card-img-top mb-5 mb-md-0 img" src="{{ vich_uploader_asset(produit,'imageFile')}}" alt="...">
                </div>

                <div class="col-md-6 text">
                
                <h1 class="display-5 fw-bolder">{{ produit.nom }}</h1>
                <div class="fs-5 mb-5">
                    {# <span class="text-decoration-line-through">{{ produit.technologie}}</span> #}
                    <span>{{ produit.technologie}}</span>
                </div>
                <p class="lead">{{ produit.description}}</p>
                
                    
                    
                    {# <a  href="/"><i class="fa-brands fa-github"></i></a> #}
                
                </div>
            </div>
            {% endfor %}
        </div>
    </div>
</section>
{# {% endblock %} #}
